    <methods>
        <detection use="true">
            <method>HierarchicalNormalSpaceSampling</method>
            <properties>
                <nSamples>1000</nSamples>
                <nLevels>10</nLevels>
            </properties>
        </detection>

        <description use="false">
            <method>SHOT</method>
            <properties>
                <radiusNormalFactor>5</radiusNormalFactor>
                <radiusSearchFactor>20</radiusSearchFactor>
                <nNeighbours>100</nNeighbours>
            </properties>
        </description>
<!--
